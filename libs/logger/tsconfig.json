{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Node 17",

	"compilerOptions": {
		"lib": ["dom", "es2021"],
		"types": ["node"],
		"module": "commonjs",
		"target": "es2021",
		"noEmit": false,
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"strictNullChecks": true
	}
}
