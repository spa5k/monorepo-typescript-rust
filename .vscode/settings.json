{
	"search.exclude": {
		"**/.yarn": true,
		"**/.pnp.*": true
	},
	"[javascript]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[typescript]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[json]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[markdown]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[toml]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"[rust]": {
		"editor.defaultFormatter": "matklad.rust-analyzer"
	},
	"conventionalCommits.scopes": [
		"main",
		"commit",
		"@gaia/routes",
		"deps",
		"@gaia",
		"@gaia/logs",
		"@athena",
		"docs"
	]
}
